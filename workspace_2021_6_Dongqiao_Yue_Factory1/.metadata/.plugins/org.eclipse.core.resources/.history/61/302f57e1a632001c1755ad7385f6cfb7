package as3;

public class ElectronicItemFactory  {
	private static ElectronicItemFactory Item;
	 
	   
	  ElectronicItemFactory(){}
	 
	   public String getItem(int ID, String Name, int Price){
		   if (Item == null) {  
			   Item = new ElectronicItemFactory();  
		    }  
		    return "ID: " + ID + " Name: " + Name + " Price: " + Price;  
		    }



	   

	 
}

