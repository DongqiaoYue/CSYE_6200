package as3;

public class Item {
	private int ID;

	private String Name;	
	
	private int Price;
	
	public Item(int ID, String Name, int Price) {
		this.ID = ID;
		this.Name = Name;
		this.Price = Price;
	}
	
	public int getID() {
		return ID;
	}
	public void setID(int ID) {
		this.ID = ID;
	}
	
	public String getName() {
		return Name;
	}
	
	public void setName(String Name) {
		this.Name = Name;
	}
	
	public int getPrice() {
		return Price;
	}
	public void setPrice(int Price) {
		this.Price = Price;
	}

}
