package as3;

public class ElectronicItem implements Item{

	private int ID;
	private String Name;
	private int Price;

	public ElectronicItem() {
		this.ID = ID;
		this.Name = Name;
		this.Price = Price;

	}
	

	public int getID() {
		return ID;
	}
	
	public String getName() {
		return Name;
	}
	
	public int getPrice() {
		return Price;
	}
	
	@Override
	public void Send() {
		System.out.println("ID: " + ID + " Name " + Name + " Price: " + Price );
	}

}
