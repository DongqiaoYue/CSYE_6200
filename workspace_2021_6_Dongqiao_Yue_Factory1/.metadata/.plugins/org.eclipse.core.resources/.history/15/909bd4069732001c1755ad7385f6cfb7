package as3;

public class ElectronicItemFactory {
	private static ElectronicItemFactory Item;
	 
	   
	  ElectronicItemFactory(){}
	 
	   public as3.Item getItem(int ID, String Name, int Price){
		   if (Item == null) {  
			   Item = new ElectronicItemFactory();  
		    }  
		    return (as3.Item) Item;  
		    }  
	   
	   public void showMessage(){
		      System.out.println("Hello World!");
		   }
	 
}

