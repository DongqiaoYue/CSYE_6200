package as3;

public class FoodItem implements Item{
	
	private int ID;
	private String Name;
	private int Price;

	public FoodItem(int iD, String Name, int Price) {
		this.ID = ID;
		this.Name = Name;
		this.Price = Price;
	}
	

	public FoodItem() {
		// TODO Auto-generated constructor stub
	}


	public int getID() {
		return ID;
	}
	
	public String getName() {
		return Name;
	}
	
	public int getPrice() {
		return Price;
	}
	
	@Override
	public void Send() {
		System.out.println("Inside Circle::draw() method.");
	}
	
	

}
